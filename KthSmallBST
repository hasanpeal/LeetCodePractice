class Solution {
    int count = 0;
    int target = 0;
    int res = -1;
    public int kthSmallest(TreeNode root, int k) {
        target = k;
        inorder(root);
        return res;
    }
    public void inorder(TreeNode root){
        if(root == null) return;
        inorder(root.left);
        count++;
        if(count == target){
            res = root.val;
            return;
        }

        inorder(root.right);
    }
}
